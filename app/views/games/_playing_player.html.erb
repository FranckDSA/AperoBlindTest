<%= simple_form_for [ @game, @answer ], defaults: {label: false} do |f| %>
  <%= f.submit "Buzz !", class: "buzz gros-buzzer"%>
<% end %>

<!-- <p>Mon score: <%#= @current_player.score %></p> -->

<h2>Scores</h2>
<p>
  <% @game.players.each do |player| %>
  <li><%= player.user_name %> : <%= player.score %></li>
  <% end %>
</p>
